&НаКлиенте
// Обработчик нажатия на гиперссылку "История синхронизации"
//
Процедура MRS_ИсторияСинхронизацииНажатие(Элемент)
	
	Если НЕ ЗначениеЗаполнено(Объект.Ссылка) Тогда
		
		ПоказатьПредупреждение(, "Сначала необходимо сохранить элемент!");
		Возврат;
		
	КонецЕсли;
	
	// Открываем регистр сведений с отбором по текущей позиции меню
	ПараметрыФормы = Новый Структура;
	ПараметрыФормы.Вставить("Отбор", Новый Структура("ПозицияМеню", Объект.Ссылка));
	
	ОткрытьФорму(
		"РегистрСведений.MRS_ИсторияСинхронизацииМенюSimphony.ФормаСписка",
		ПараметрыФормы,
		ЭтотОбъект,
		УникальныйИдентификатор
	);
	
КонецПроцедуры

&НаСервере
// Настраивает условное оформление для статуса синхронизации
//
Процедура MRS_НастроитьУсловноеОформлениеСтатуса()
	
	// Очищаем существующее условное оформление для статуса
	УсловноеОформление.Элементы.Очистить();
	
	// НеВыгружено - Синий
	ЭлементУО = УсловноеОформление.Элементы.Добавить();
	
	ОтборЭлемента = ЭлементУО.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("Объект.MRS_СтатусСинхронизацииSimphony");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Перечисления.MRS_СтатусыСинхронизацииSimphony.НеВыгружено;
	ОтборЭлемента.Использование = Истина;
	
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("ЦветТекста", WebЦвета.СинийСтальной);
	
	ПолеЭлемента = ЭлементУО.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("MRS_СтатусСинхронизацииSimphony");
	ПолеЭлемента.Использование = Истина;
	
	// ВОчереди - Оранжевый
	ЭлементУО = УсловноеОформление.Элементы.Добавить();
	
	ОтборЭлемента = ЭлементУО.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("Объект.MRS_СтатусСинхронизацииSimphony");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Перечисления.MRS_СтатусыСинхронизацииSimphony.ВОчереди;
	ОтборЭлемента.Использование = Истина;
	
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("ЦветТекста", WebЦвета.Оранжевый);
	
	ПолеЭлемента = ЭлементУО.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("MRS_СтатусСинхронизацииSimphony");
	ПолеЭлемента.Использование = Истина;
	
	// Выгружено - Зеленый
	ЭлементУО = УсловноеОформление.Элементы.Добавить();
	
	ОтборЭлемента = ЭлементУО.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("Объект.MRS_СтатусСинхронизацииSimphony");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Перечисления.MRS_СтатусыСинхронизацииSimphony.Выгружено;
	ОтборЭлемента.Использование = Истина;
	
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("ЦветТекста", WebЦвета.Зеленый);
	
	ПолеЭлемента = ЭлементУО.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("MRS_СтатусСинхронизацииSimphony");
	ПолеЭлемента.Использование = Истина;
	
	// Ошибка - Оранжево-красный
	ЭлементУО = УсловноеОформление.Элементы.Добавить();
	
	ОтборЭлемента = ЭлементУО.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("Объект.MRS_СтатусСинхронизацииSimphony");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Перечисления.MRS_СтатусыСинхронизацииSimphony.Ошибка;
	ОтборЭлемента.Использование = Истина;
	
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("ЦветТекста", WebЦвета.ОранжевоКрасный);
	
	ПолеЭлемента = ЭлементУО.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("MRS_СтатусСинхронизацииSimphony");
	ПолеЭлемента.Использование = Истина;
	
	// ТребуетВнимания - Красный, жирный
	ЭлементУО = УсловноеОформление.Элементы.Добавить();
	
	ОтборЭлемента = ЭлементУО.Отбор.Элементы.Добавить(Тип("ЭлементОтбораКомпоновкиДанных"));
	ОтборЭлемента.ЛевоеЗначение = Новый ПолеКомпоновкиДанных("Объект.MRS_СтатусСинхронизацииSimphony");
	ОтборЭлемента.ВидСравнения = ВидСравненияКомпоновкиДанных.Равно;
	ОтборЭлемента.ПравоеЗначение = Перечисления.MRS_СтатусыСинхронизацииSimphony.ТребуетВнимания;
	ОтборЭлемента.Использование = Истина;
	
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("ЦветТекста", WebЦвета.Красный);
	ЭлементУО.Оформление.УстановитьЗначениеПараметра("Жирный", Истина);
	
	ПолеЭлемента = ЭлементУО.Поля.Элементы.Добавить();
	ПолеЭлемента.Поле = Новый ПолеКомпоновкиДанных("MRS_СтатусСинхронизацииSimphony");
	ПолеЭлемента.Использование = Истина;
	
КонецПроцедуры

